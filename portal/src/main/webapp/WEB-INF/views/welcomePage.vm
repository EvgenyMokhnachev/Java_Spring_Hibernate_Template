<div class="modalWindow loginModal">
    <div class="content">
        <button class="closeModalBtn"></button>

        <div class="modalRow">
            <div class="title">Login Your ATM Account</div>
        </div>

        <form action="" onsubmit="return false">
            <div class="modalRow">
                <input type="email" class="email notEmpty" placeholder="Email"/>
                <div class="errorMessage" data-text="Email address not correct"></div>
            </div>

            <div class="modalRow">
                <input type="password" class="password notEmpty" placeholder="Password"/>
                <div class="errorMessage" data-text="Password not correct"></div>
            </div>

            <button class="actionBtn logInBtn">LOG IN</button>
        </form>

        <div class="modalFooter">
            <div class="forgotPassword">Forgot Password?</div>
            <div class="getStarted">Don't have an account? Get Started</div>
        </div>
    </div>
</div>

<div class="modalWindow registerModal">
    <div class="content">
        <button class="closeModalBtn"></button>

        <div class="modalRow">
            <div class="title">Get Started Absolutely <span style="color: #0089fe; font-weight: bold">FREE</span></div>
            <div class="modalDescription">No credit card needed.</div>
        </div>

        <form action="" onsubmit="return false">
            <div class="modalRow">
                <input type="text" class="username notEmpty ajax" data-href="user/checkUsernameAvailable" data-key="username" placeholder="Your Name"/>
                <div class="errorMessage" data-text="Username not correct"></div>
            </div>

            <div class="modalRow">
                <input type="email" class="email notEmpty ajax" data-href="user/checkEmailAvailable" data-key="email" placeholder="Your Email Address"/>
                <div class="errorMessage" data-text="Email address not correct"></div>
            </div>

            <div class="modalRow">
                <input type="password" class="password notEmpty" placeholder="Create a Password"/>
                <div class="errorMessage" data-text="Password not correct"></div>
            </div>

            <div class="modalRow">
                <button class="actionBtn signUpBtn">SIGN UP FREE</button>
            </div>
        </form>

        <div class="modalRow">
            <div class="modalDescription">By clicking “Sign Up Free” I agree to AudioTaskManager’s <button class="termsOfServiceBtn">Terms of Service</button></div>
        </div>

        <div class="modalFooter">
            <div class="alreadyAccountText">Already have an account? Login!</div>
        </div>
    </div>
</div>

<div class="modalWindow resetPasswordModal">
    <div class="content">
